<resources>
    <string name="app_name">ThirtyTipsCalisthenics</string>
    <string name="tip1">Foundation First</string>
    <string name="description_title1">Begin the calisthenics routine</string>
    <string name="description1">
        • 3×10 push-ups\n
        • 3×20 air squats\n
        • 3×15 crunches\n
        Focus on form over speed\n
    </string>
    <string name="nutrition1">Nutrition: Eat clean</string>
    <string name="nutrition_description1">
        Breakfast: oatmeal + banana\n
        Lunch: grilled chicken + rice + veggies\n
        Dinner: tuna salad + olive oil dressing\n
    </string>

    <string name="tip2">Core Awakening</string>
